{
  "compilerOptions": {
    "target": "esnext",
    "module": "commonjs",
    "jsx": "react-native",
    "strict": true,
    "moduleResolution": "node",
    "esModuleInterop": true,
    "skipLibCheck": true,
    "forceConsistentCasingInFileNames": true
  },
  "include": [
    "node_modules",
    "app/**/*.tsx",
    "expo-env.d.ts",
    ".expo/types/**/*.ts"
, "app/(Tabs)/HomeScreen.js", "app/SignUp.js", "app/SignIn.js", "app/index.js", "app/(Tabs)/students/Fee.js"  ],
  "exclude": [
    "node_modules",
    "babel.config.js",
    "metro.config.js",
    "jest.config.js"
  ],
  "extends": "expo/tsconfig.base"
}
